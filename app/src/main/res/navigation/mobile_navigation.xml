<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/fragment_home">
    <fragment
        android:id="@+id/fragment_home"
        android:name="com.skhproject.classifiedapp.ui.fragment.home.HomeFragment"
        tools:layout="@layout/home_fragment">
        <action
            android:id="@+id/action_fragment_home_to_fragment_details"
            app:destination="@id/fragment_details" />
    </fragment>
    <fragment
        android:id="@+id/fragment_details"
        android:name="com.skhproject.classifiedapp.ui.fragment.detail.DetailFragment"
        tools:layout="@layout/detail_fragment"/>
</navigation>